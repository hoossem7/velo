{% extends 'back.html.twig' %}

{% block title %}Location{% endblock %}

{% block body %}
    <h1>détails de réservation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ location.id }}</td>
            </tr>
            <tr>
                <th>Id_u</th>
                <td>{{ location.idU }}</td>
            </tr>
            <tr>
                <th>Start_date</th>
                <td>{{ location.startDate }}</td>
            </tr>
            <tr>
                <th>End_date</th>
                <td>{{ location.endDate }}</td>
            </tr>
            <tr>
                <th>Prix</th>
                <td>{{ location.prix }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_location_index') }}">back to list</a>

    <a href="{{ path('app_location_edit', {'id': location.id}) }}">edit</a>

    {{ include('location/_delete_form.html.twig') }}
{% endblock %}
